[6.486429, "o", "\r\n\u001b[1m\u001b[32m🌳 tree2dir - Directory Structure Generator \u001b[0mv1.2.0\r\n\r\n"]
[6.48667, "o", "\u001b[1m\u001b[36mUsage:\u001b[0m\r\n"]
[6.48668, "o", "  \u001b[33mtree2dir\u001b[0m [options]\r\n"]
[6.486691, "o", "  \u001b[33mtree2dir generate\u001b[0m [options]\r\n"]
[6.486803, "o", "\r\n\u001b[1m\u001b[36mOptions:\u001b[0m\r\n"]
[6.486838, "o", "  \u001b[32m-f, --file\u001b[0m <path>      📄 Path to an ASCII tree file\r\n"]
[6.486845, "o", "  \u001b[32m-g, --gist\u001b[0m <gistUrl>   🔗 URL of a GitHub gist containing the ASCII tree\r\n"]
[6.48686, "o", "  \u001b[32m-o, --output\u001b[0m <dir>     📁 Specify the output directory (default: current directory)\r\n"]
[6.486893, "o", "  \u001b[32m--dry-run\u001b[0m              🔍 Visualize the structure without creating files\r\n"]
[6.486906, "o", "  \u001b[32m--debug\u001b[0m                🐞 Show debug information during execution\r\n"]
[6.486921, "o", "  \u001b[32m-h, --help\u001b[0m             ℹ️  Show this help message\r\n"]
[6.487031, "o", "\r\n\u001b[1m\u001b[36mExamples:\u001b[0m\r\n"]
[6.487039, "o", "  \u001b[33mtree2dir generate -f mytree.txt -o ./my-project\u001b[0m\r\n"]
[6.487094, "o", "  \u001b[33mtree2dir generate -g https://gist.github.com/username/gistid --dry-run\u001b[0m\r\n  \u001b[33mtree2dir generate\u001b[0m      (interactive mode)\r\n\r\n\u001b[2mFor more information, visit: https://github.com/solancer/tree2dir\u001b[0m\r\n\r\n"]
